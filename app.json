{
  "expo": {
    "name": "MyTodo",
    "slug": "MyTodo",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover", 
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.test.mytodo"
    },
    "android": {
      "package": "com.test.mytodo"
    },
    "description": "todo app made with react native",
    "githubUrl": "https://github.com/pms8212/react-native-todo.git"
  }
}
